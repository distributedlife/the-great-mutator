    "ok-selector": "^1.0.0"
  },
  "peerDependencies": {
    "immutable": "^3.8.1"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-register": "^6.14.0",
    "eslint": "^3.4.0",
    "expect": "^1.20.2",
    "immutable": "^3.8.1",
    "sinon": "^1.17.5"
