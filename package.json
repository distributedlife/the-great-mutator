  "scripts": {
    "test": "mocha --compilers js:babel-register -c -S -R spec **/*spec*.js",
    "predist": "rm -rf lib",
    "prepublish": "babel src --out-dir lib --sourceMaps inline --comments --copy-files",
    "lint": "eslint . --ext .js"
