  ],
  "author": "Ryan Boucher",
  "license": "BSD-2-Clause",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/distributedlife/the-great-mutator.git"
  },
  "bugs": {
    "url": "https://github.com/distributedlife/the-great-mutator/issues"
  },
  "homepage": "https://github.com/distributedlife/the-great-mutator#readme",
  "dependencies": {
    "is-promise": "^2.1.0",
